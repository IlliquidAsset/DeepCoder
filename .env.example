# DeepCoder CLI Configuration

# Model Host Platform (togetherai or lightningai)
MODEL_HOST_PLATFORM=togetherai

# Together.ai API Key (required if using Together.ai)
TOGETHER_API_KEY=your_together_api_key_here

# Lightning AI Configuration (required if using Lightning AI)
LIGHTNING_ENDPOINT_URL=https://your.lightning.ai/endpoint
LIGHTNING_API_KEY=your_lightning_api_key_here

# Logging Configuration
DEEPCODER_LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL

# Git Integration (true/false)
DEEPCODER_GIT_AUTO_STAGE=false
DEEPCODER_GIT_AUTO_COMMIT=false